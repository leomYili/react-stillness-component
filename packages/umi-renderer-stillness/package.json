{
  "name": "umi-renderer-stillness",
  "version": "0.1.3",
  "description": "react-stillness-component renderer",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist"
  },
  "keywords": [
    "keep-alive",
    "stillness",
    "react-stillness-component"
  ],
  "author": "leomyili",
  "license": "MIT",
  "dependencies": {
    "@types/react": "^17.x",
    "@types/react-dom": "^17.x",
    "@types/react-router-config": "^5.0.2",
    "@umijs/runtime": "^3.x",
    "react-router-config": "5.1.1",
    "react-stillness-component": "^0.6.3"
  },
  "peerDependencies": {
    "react": "16.x || 17.x",
    "react-dom": "16.x || 17.x"
  },
  "rollup": {
    "input": "src/index.ts",
    "output": [
      {
        "file": "dist/index.js",
        "format": "umd",
        "name": "StillnessUmiRenderer",
        "globals": {
          "react": "React",
          "react-dom": "ReactDOM",
          "runtime": "runtime"
        }
      }
    ],
    "external": [
      "react",
      "react-dom",
      "@umijs/runtime"
    ]
  }
}
